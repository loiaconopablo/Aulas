<h1>Nueva Consulta para la oferta <%=@pedido.aula.nombre %> </h1> 

<% form_for(@pedido, '/consulta/create') do |f| %>
 
  <%= partial 'pedidos/form', :locals => { :f => f } %>
	
<input type="hidden" name="aula_id" value="<%= @pedido.aula.id %>" />
  <%= f.submit "Create", :class => "btn btn-primary" %>
  <%= link_to 'Cancel', url_for(:pedidos, :listar), :class => "btn btn-secondary" %>
<% end %>
